import java.util.Scanner;
class D32FactorialTest {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("숫자를 입력해주세요 : ");
		int n = sc.nextInt();
		int fac = 1;
		for(int i=1;i<n+1;i++){
			fac *= i;
		}
		System.out.printf("%d!은 %d입니다.", n, fac);
	}
}
